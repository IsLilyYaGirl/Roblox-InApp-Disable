{
    "manifest_version": 2,
    "name": "Roblox-InApp-Disable",
    "version": "0.0.1",
    "description": "Roblox-InApp-Disable",
    "web_accessible_resources": [
        "window.js"
    ],
    "content_scripts": [
        {
            "matches": ["*://*.roblox.com/games/*"],
            "js": ["script.js"],
            "run_at": "document_end"
        }
    ]
}